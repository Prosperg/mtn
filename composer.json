{
    "name": "pkl/mtn-momo-sdk",
    "description": "Un package Laravel pour faciliter l'intégration du moyen de paiement électronique de l'opérateur MTN",
    "version": "1.0.1",
    "type": "library",
    "require": {
        "php": "^7.3 || ^8.0 || ^8.1 || ^8.2 || ^8.3",
        "illuminate/support": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pkl\\Mtn\\MomoSdk\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pkl\\Mtn\\MomoSdk\\MtnMomoServiceProvider"
            ],
            "aliases": {
                "MomoSdk": "Pkl\\Mtn\\MomoSdk\\Facades\\MomoSdk"
            }
        }
    },
    "authors": [
        {
            "name": "Koffi Prosper GOUTO",
            "email": "promasdev@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
